{
  "id": "register-a-helicopter",
  "name": "Register a helicopter",
  "landingPage": {
    "title": "Register a helicopter",
    "lead": "Use this service to register a civilian helicopter based in the UK.",
    "sections": [
      {
        "type": "heading",
        "content": "What you can do",
        "level": "m"
      },
      {
        "type": "paragraph",
        "content": "You can use this service to apply for a new UK registration mark for a civilian helicopter."
      },
      {
        "type": "paragraph",
        "content": "You cannot use this service to register military aircraft or drones."
      },
      {
        "type": "heading",
        "content": "What you’ll need",
        "level": "m"
      },
      {
        "type": "list",
        "content": [
          "Helicopter manufacturer and model",
          "Serial number",
          "Your name and contact details"
        ],
        "listType": "bullet"
      },
      {
        "type": "heading",
        "content": "How long it takes",
        "level": "m"
      },
      {
        "type": "paragraph",
        "content": "The application takes about 5 minutes to complete."
      }
    ],
    "startButtonText": "Start now",
    "startButtonHref": "/civil-aviation-authority/register-a-helicopter/apply"
  },
  "startPage": "applicant-details",
  "pages": {
    "applicant-details": {
      "id": "applicant-details",
      "title": "Your details",
      "components": [
        {
          "type": "paragraph",
          "id": "applicant-details-intro",
          "props": {
            "text": "Tell us who is responsible for this registration."
          }
        },
        {
          "type": "textInput",
          "id": "applicant-full-name",
          "props": {
            "id": "applicant-full-name",
            "name": "applicantFullName",
            "label": "Full name",
            "hint": "Enter your full legal name.",
            "type": "text",
            "autocomplete": "name",
            "width": "full",
            "disabled": false,
            "readonly": false,
            "spellcheck": true
          }
        },
        {
          "type": "textInput",
          "id": "applicant-email",
          "props": {
            "id": "applicant-email",
            "name": "applicantEmail",
            "label": "Email address",
            "hint": "We’ll use this to send you confirmation of your registration.",
            "type": "email",
            "autocomplete": "email",
            "width": "full",
            "disabled": false,
            "readonly": false,
            "spellcheck": false
          }
        },
        {
          "type": "textInput",
          "id": "applicant-phone",
          "props": {
            "id": "applicant-phone",
            "name": "applicantPhone",
            "label": "Telephone number",
            "hint": "Include the country code if outside the UK.",
            "type": "tel",
            "autocomplete": "tel",
            "width": "20",
            "disabled": false,
            "readonly": false,
            "spellcheck": false
          }
        }
      ],
      "nextPage": "helicopter-details"
    },
    "helicopter-details": {
      "id": "helicopter-details",
      "title": "Helicopter details",
      "components": [
        {
          "type": "textInput",
          "id": "helicopter-manufacturer",
          "props": {
            "id": "helicopter-manufacturer",
            "name": "helicopterManufacturer",
            "label": "Manufacturer",
            "hint": "For example, Airbus, Leonardo, Bell.",
            "type": "text",
            "width": "full",
            "disabled": false,
            "readonly": false,
            "spellcheck": true
          }
        },
        {
          "type": "textInput",
          "id": "helicopter-model",
          "props": {
            "id": "helicopter-model",
            "name": "helicopterModel",
            "label": "Model",
            "hint": "For example, H145, AW109.",
            "type": "text",
            "width": "full",
            "disabled": false,
            "readonly": false,
            "spellcheck": true
          }
        },
        {
          "type": "textInput",
          "id": "helicopter-serial-number",
          "props": {
            "id": "helicopter-serial-number",
            "name": "helicopterSerialNumber",
            "label": "Serial number",
            "hint": "You can find this on the helicopter’s data plate or documents.",
            "type": "text",
            "width": "20",
            "disabled": false,
            "readonly": false,
            "spellcheck": false
          }
        }
      ],
      "nextPage": "based-in-uk",
      "previousPage": "applicant-details"
    },
    "based-in-uk": {
      "id": "based-in-uk",
      "title": "Is the helicopter based in the UK?",
      "components": [
        {
          "type": "radios",
          "id": "based-in-uk-radios",
          "props": {
            "id": "based-in-uk-radios",
            "name": "basedInUk",
            "legend": "Where is the helicopter normally based?",
            "hint": "Select yes if it is mainly operated from the UK.",
            "items": [
              {
                "value": "yes",
                "text": "Yes, it is mainly based in the UK",
                "checked": false,
                "disabled": false
              },
              {
                "value": "no",
                "text": "No, it is mainly based outside the UK",
                "checked": false,
                "disabled": false
              }
            ]
          }
        }
      ],
      "nextPage": "check-answers",
      "previousPage": "helicopter-details"
    },
    "check-answers": {
      "id": "check-answers",
      "title": "Check your answers",
      "components": [
        {
          "type": "heading",
          "id": "check-answers-heading",
          "props": {
            "text": "Check your answers before submitting",
            "size": "l"
          }
        },
        {
          "type": "paragraph",
          "id": "check-answers-intro",
          "props": {
            "text": "Make sure the details you have given are correct. You can go back to change anything."
          }
        }
      ],
      "nextPage": "confirmation",
      "previousPage": "based-in-uk"
    },
    "confirmation": {
      "id": "confirmation",
      "title": "Application submitted",
      "components": [
        {
          "type": "insetText",
          "id": "confirmation-reference",
          "props": {
            "text": "We’ve received your application to register a helicopter."
          }
        },
        {
          "type": "paragraph",
          "id": "confirmation-next-steps",
          "props": {
            "text": "We’ll email you within 5 working days to confirm your registration or ask for more information."
          }
        },
        {
          "type": "paragraph",
          "id": "confirmation-contact",
          "props": {
            "text": "If you do not receive an email within 5 working days, contact the Civil Aviation Authority with your name and helicopter serial number."
          }
        }
      ],
      "previousPage": "check-answers"
    }
  },
  "checkYourAnswersPage": "check-answers",
  "completionPage": "confirmation"
}
