{
  "id": "register-a-helicopter",
  "name": "Register a helicopter",
  "landingPage": {
    "title": "Register a helicopter",
    "lead": "Use this service to register a civil helicopter for use in the UK.",
    "sections": [
      {
        "type": "heading",
        "content": "Before you start",
        "level": "m"
      },
      {
        "type": "paragraph",
        "content": "You can use this service to register a helicopter that is based in the UK and used for civil (non-military) purposes."
      },
      {
        "type": "list",
        "content": [
          "you must be the owner or have permission from the owner",
          "you need basic details about the helicopter",
          "registration usually takes less than 10 minutes"
        ],
        "listType": "bullet"
      },
      {
        "type": "heading",
        "content": "What you’ll need",
        "level": "m"
      },
      {
        "type": "list",
        "content": [
          "helicopter manufacturer and model",
          "helicopter serial number (if known)",
          "your contact details"
        ],
        "listType": "bullet"
      }
    ],
    "startButtonText": "Start now",
    "startButtonHref": "/civil-aviation-authority/register-a-helicopter/apply"
  },
  "startPage": "start",
  "pages": {
    "start": {
      "id": "start",
      "title": "Register a helicopter",
      "components": [
        {
          "type": "paragraph",
          "id": "start-intro",
          "props": {
            "text": "Answer a few quick questions to register a helicopter in the UK."
          }
        }
      ],
      "nextPage": "helicopter-details"
    },
    "helicopter-details": {
      "id": "helicopter-details",
      "title": "Helicopter details",
      "components": [
        {
          "type": "heading",
          "id": "helicopter-details-heading",
          "props": {
            "text": "Enter helicopter details",
            "size": "l"
          }
        },
        {
          "type": "textInput",
          "id": "helicopter-make",
          "props": {
            "id": "helicopter-make",
            "name": "helicopterMake",
            "label": "Manufacturer",
            "hint": "For example, Airbus, Bell, Leonardo",
            "type": "text",
            "autocomplete": "off",
            "width": "20",
            "disabled": false,
            "readonly": false,
            "spellcheck": true
          }
        },
        {
          "type": "textInput",
          "id": "helicopter-model",
          "props": {
            "id": "helicopter-model",
            "name": "helicopterModel",
            "label": "Model",
            "hint": "For example, H145, 407, AW109",
            "type": "text",
            "autocomplete": "off",
            "width": "20",
            "disabled": false,
            "readonly": false,
            "spellcheck": true
          }
        },
        {
          "type": "textInput",
          "id": "helicopter-serial",
          "props": {
            "id": "helicopter-serial",
            "name": "helicopterSerial",
            "label": "Serial number (if known)",
            "hint": "You can leave this blank if you do not know it",
            "type": "text",
            "autocomplete": "off",
            "width": "20",
            "disabled": false,
            "readonly": false,
            "spellcheck": false
          }
        }
      ],
      "nextPage": "owner-details",
      "previousPage": "start"
    },
    "owner-details": {
      "id": "owner-details",
      "title": "Owner details",
      "components": [
        {
          "type": "heading",
          "id": "owner-details-heading",
          "props": {
            "text": "Who owns the helicopter?",
            "size": "l"
          }
        },
        {
          "type": "textInput",
          "id": "owner-full-name",
          "props": {
            "id": "owner-full-name",
            "name": "ownerFullName",
            "label": "Full name",
            "hint": "Enter the name of the person or organisation that owns the helicopter",
            "type": "text",
            "autocomplete": "name",
            "width": "full",
            "disabled": false,
            "readonly": false,
            "spellcheck": true
          }
        },
        {
          "type": "textInput",
          "id": "owner-email",
          "props": {
            "id": "owner-email",
            "name": "ownerEmail",
            "label": "Email address",
            "hint": "We’ll use this to send your registration confirmation",
            "type": "email",
            "autocomplete": "email",
            "width": "full",
            "disabled": false,
            "readonly": false,
            "spellcheck": false
          }
        },
        {
          "type": "textInput",
          "id": "owner-phone",
          "props": {
            "id": "owner-phone",
            "name": "ownerPhone",
            "label": "Telephone number",
            "hint": "Enter a UK telephone number, for example 07700 900123",
            "type": "tel",
            "autocomplete": "tel",
            "width": "20",
            "disabled": false,
            "readonly": false,
            "spellcheck": false
          }
        }
      ],
      "nextPage": "check-your-answers",
      "previousPage": "helicopter-details"
    },
    "check-your-answers": {
      "id": "check-your-answers",
      "title": "Check your answers",
      "components": [
        {
          "type": "heading",
          "id": "cya-heading",
          "props": {
            "text": "Check your answers before you submit",
            "size": "xl"
          }
        },
        {
          "type": "paragraph",
          "id": "cya-intro",
          "props": {
            "text": "Make sure the details you have given are correct. You can go back to change anything."
          }
        }
      ],
      "nextPage": "confirmation",
      "previousPage": "owner-details"
    },
    "confirmation": {
      "id": "confirmation",
      "title": "Registration submitted",
      "components": [
        {
          "type": "insetText",
          "id": "confirmation-reference",
          "props": {
            "text": "We’ve received your helicopter registration. We’ll email you a confirmation and registration reference."
          }
        },
        {
          "type": "paragraph",
          "id": "confirmation-next",
          "props": {
            "text": "If we need more information, we’ll contact you using the details you provided."
          }
        }
      ],
      "previousPage": "check-your-answers"
    }
  },
  "checkYourAnswersPage": "check-your-answers",
  "completionPage": "confirmation"
}
